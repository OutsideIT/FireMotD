{
  "firemotd": {
    "properties": {
      "theme": {
        "properties": {
          "name": "Basic",
          "creator": "Willem D'Haese",
          "created": "2020-06-08",
          "modified": "2020-06-20",
          "description": "A basic FireMotd theme",
          "version": "0.2"
        },
        "defaults": {
          "character": "#",
          "charcolor": "\\e[38;5;93m",
          "charstart": 0,
          "length": 79,
          "keycolor": "\\e[38;5;164m",
          "keystart": 4,
          "separator": "=>",
          "separatorcolor": "\\e[38;5;57m",
          "valuecolor": "\\e[38;5;207m",
          "highlightcolor": "\\e[38;5;219m"
        }
      },
      "data": [
        {
          "row": {
            "properties": {
              "type": "dynamic",
              "character": "/",
              "charcolor": "\\e[38;5;54m",
              "charstart": 5,
              "length": 74
            }
          }
        },
        {
          "row": {
            "properties": {
              "type": "dynamic",
              "character": "<=",
              "charcolor": "\\e[38;5;54m",
              "charstart": 4,
              "length": 69
            }
          }
        },
        {
          "row": {
            "properties": {
              "type": "dynamic",
              "character": "\\",
              "charcolor": "\\e[38;5;55m",
              "charstart": 5,
              "length": 74
            }
          }
        },
        {
          "row": {
            "properties": {
              "type": "dynamic",
							"character": ">",
              "charcolor": "\\e[38;5;54m",
              "data": "${host.name}"
            }
          }
        },
        {
          "row": {
            "properties": {
              "type": "dynamic",
              "character": "/",
              "charcolor": "\\e[38;5;56m",
              "charstart": 15,
              "length": 64
            }
          }
        },
        {
          "row": {
            "properties": {
              "type": "dynamic",
              "character": "/",
              "charcolor": "\\e[38;5;54m"
            }
          }
        }
      ]
    }
	}
}
